CREATE TABLE ENREGISTREMENT (ENREGISTREMENT_ID BIGINT NOT NULL, DESCRIPTION VARCHAR(255), DUREE INTEGER NOT NULL, NOM VARCHAR(255), REPETITION INTEGER NOT NULL, CATEGORIE_ID VARCHAR(255), PRIMARY KEY (ENREGISTREMENT_ID))
CREATE TABLE CATEGORIE (CATEGORIE_ID VARCHAR(255) NOT NULL, DESCRIPTION VARCHAR(255) NOT NULL, PRIMARY KEY (CATEGORIE_ID))
CREATE TABLE FRAME (NUMERO INTEGER NOT NULL, ENREGISTREMENT_ID BIGINT NOT NULL, PRIMARY KEY (NUMERO, ENREGISTREMENT_ID))
CREATE TABLE JOINTURE (JOINTURE_ID VARCHAR(255) NOT NULL, NOM VARCHAR(255) NOT NULL, W FLOAT NOT NULL, WX FLOAT NOT NULL, WY FLOAT NOT NULL, WZ FLOAT NOT NULL, X FLOAT NOT NULL, Y FLOAT NOT NULL, Z FLOAT NOT NULL, FRAME_ID INTEGER NOT NULL, ENREGISTREMENT_ID BIGINT NOT NULL, PRIMARY KEY (JOINTURE_ID, FRAME_ID, ENREGISTREMENT_ID))
ALTER TABLE ENREGISTREMENT ADD CONSTRAINT FK_ENREGISTREMENT_CATEGORIE_ID FOREIGN KEY (CATEGORIE_ID) REFERENCES CATEGORIE (CATEGORIE_ID)
ALTER TABLE FRAME ADD CONSTRAINT FK_FRAME_ENREGISTREMENT_ID FOREIGN KEY (ENREGISTREMENT_ID) REFERENCES ENREGISTREMENT (ENREGISTREMENT_ID)
ALTER TABLE JOINTURE ADD CONSTRAINT FK_JOINTURE_FRAME_ID FOREIGN KEY (FRAME_ID, ENREGISTREMENT_ID) REFERENCES FRAME (NUMERO, ENREGISTREMENT_ID)
