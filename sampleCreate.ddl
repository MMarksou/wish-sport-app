CREATE TABLE ENREGISTREMENT (NOM VARCHAR NOT NULL, DESCRIPTION VARCHAR, DUREE INTEGER NOT NULL, REPETITION INTEGER NOT NULL, CATEGORIE_ID VARCHAR NOT NULL, PRIMARY KEY (NOM, CATEGORIE_ID))
CREATE TABLE CATEGORIE (CATEGORIE_ID VARCHAR NOT NULL, DESCRIPTION VARCHAR NOT NULL, PRIMARY KEY (CATEGORIE_ID))
CREATE TABLE FRAME (NUMERO INTEGER NOT NULL, CATEGORIE_ID VARCHAR NOT NULL, ENREGISTREMENT_ID VARCHAR NOT NULL, PRIMARY KEY (NUMERO, CATEGORIE_ID, ENREGISTREMENT_ID))
CREATE TABLE JOINTURE (ID VARCHAR NOT NULL, NOM VARCHAR NOT NULL, W DOUBLE NOT NULL, WX DOUBLE NOT NULL, WY DOUBLE NOT NULL, WZ DOUBLE NOT NULL, X DOUBLE NOT NULL, Y DOUBLE NOT NULL, Z DOUBLE NOT NULL, FRAME_ID INTEGER NOT NULL, CATEGORIE_ID VARCHAR NOT NULL, ENREGISTREMENT_ID VARCHAR NOT NULL, PRIMARY KEY (ID, FRAME_ID, CATEGORIE_ID, ENREGISTREMENT_ID))
ALTER TABLE ENREGISTREMENT ADD CONSTRAINT FK_ENREGISTREMENT_CATEGORIE_ID FOREIGN KEY (CATEGORIE_ID) REFERENCES CATEGORIE (CATEGORIE_ID)
ALTER TABLE FRAME ADD CONSTRAINT FK_FRAME_ENREGISTREMENT_ID FOREIGN KEY (ENREGISTREMENT_ID, CATEGORIE_ID) REFERENCES ENREGISTREMENT (NOM, CATEGORIE_ID)
ALTER TABLE JOINTURE ADD CONSTRAINT FK_JOINTURE_FRAME_ID FOREIGN KEY (FRAME_ID, CATEGORIE_ID, ENREGISTREMENT_ID) REFERENCES FRAME (NUMERO, CATEGORIE_ID, ENREGISTREMENT_ID)
